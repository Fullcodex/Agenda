<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Agenda{% endblock %}</title>
        {% block stylesheets %}
            <link href="{{ asset('assets/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet">
            <link href="{{ asset('assets/fontawesome/css/all.min.css') }}" rel="stylesheet">
        {% endblock %}
        
        {%  block css_spe %}<link href="{{ asset('assets/special_css/signin.css') }}" rel="stylesheet">{% endblock %}

        {% block javascripts1 %}
            <script src="{{ asset('assets/jquery/jquery.min.js') }}"></script>
        {% endblock %}
    </head>
    <body>
        <style>
            .bd-placeholder-img {
                font-size: 1.125rem;
                text-anchor: middle;
                -webkit-user-select: none;
                -moz-user-select: none;
                -ms-user-select: none;
                user-select: none;
            }

            @media (min-width: 768px) {
                .bd-placeholder-img-lg {
                    font-size: 3.5rem;
                }
            }
        </style>
        {% block naviguation %}{% endblock %}
        {% block body %}{% endblock %}
        {% block javascripts2 %}
            <script src="{{ asset('assets/bootstrap/js/bootstrap.min.js') }}"></script>
        {% endblock %}
    </body>
</html>
